name = <%= themeMachineName %>
description = <%= themeHumanName %>
core = 7.x
engine = phptemplate

; ========================================
; Regions
; ========================================
regions[top_bar_right]        = Top Bar Right
regions[help]                 = Help
regions[highlighted]          = Highlighted
regions[content]              = Content
regions[sidebar_first]        = Sidebar First
regions[sidebar_second]       = Sidebar Second
regions[footer_first]         = Footer First
regions[footer_second]        = Footer Second
regions[footer_third]         = Footer Third

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = dist/assets/css/app.css

; ========================================
; Scripts
; ========================================
; The script below is being embedded
; in the bottom of the html.tpl.php file
; as per foundation js documentation.
;
;scripts[] = public/assets/js/app.js

; ========================================
; Panel Layouts
; ========================================
plugins[panels][layouts] = panels
